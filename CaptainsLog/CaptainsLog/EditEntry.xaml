<Window x:Class="CaptainsLog.EditEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CaptainsLog"
        mc:Ignorable="d"
        Title="EditEntry" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" Margin="10,33,9,10" TextWrapping="Wrap" Text="Edit entry here"/>
        <TextBox x:Name="textBoxEditTitle" HorizontalAlignment="Left" Height="23" Margin="34,5,0,0" TextWrapping="Wrap" Text="Enter Title Here" VerticalAlignment="Top" Width="224"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="4,6,0,0" TextWrapping="Wrap" Text="Title:" VerticalAlignment="Top" Height="20"/>
        <Button x:Name="buttonSaveEdit" Content="+" HorizontalAlignment="Left" Margin="263,5,0,0" VerticalAlignment="Top" Width="20" Click="buttonSaveEdit_Click" Height="20"/>

    </Grid>
</Window>
